#:kivy 2.0.0
<Authorization>:
    MDCard:
        size_hint: None, None
        size: 370, 400
        pos_hint: {"center_x" : 0.5,"center_y": 0.5}
        elevation: 10
        padding: 25
        spacing: 25
        orientation: 'vertical'
        MDBoxLayout:
            orientation: 'vertical'
            pos_hint: {'center_x':.5}
            MDLabel:
                text: 'MikroLite\nАвторизация'
                halign: 'center'
            MDTextField:
                id: ip_dns
                hint_text: "IPv4 адрес или DNS имя"
                font_size: 20
            MDTextField:
                id: user
                hint_text: "Имя пользователя"
                font_size: 20
            MDTextField:
                id: passwordv
                hint_text: "Пароль"
                font_size: 20
                password: True
            MDBoxLayout:
                orientation: 'horizontal'
                MDRaisedButton:
                    text: "Подключиться"
                    md_bg_color: .35, .35, 1, 1
                    font_size: 22
                    on_release:
                        app.connector()
                        app.resour()
                        app.root.current = 'MainMenu'
                MDLabel:
                    size_hint: 0.1, None
                MDBoxLayout:
                    orientation: 'horizontal'
                    MDRaisedButton:
                        text: "Авто вход"
                        md_bg_color: .35, .35, 1, 1
                        font_size: 22
                        on_release:
                            app.standart()
                            app.resour()
                            app.root.current = 'MainMenu'
<MainMenu>:
    BoxLayout:
        orientation:'vertical'
        MDToolbar:
            title: 'MikroLite'
            md_bg_color: .35, .35, 1, 1
            specific_text_color: 1, 1, 1, 1
            right_action_items: [["exit-to-app", lambda x: app.logout()]]
        MDBottomNavigation:
            panel_color: .35, .35, 1, 1
            MDBottomNavigationItem:
                name: 'Main_Status'
                text: 'Статус'
                ScrollView:
                    GridLayout:
                        cols: 2
                        #orientation: 'vertical'
                        size_hint_y: None
                        height: self.minimum_height
                        padding: dp(48)
                        spacing: 10
                        MDLabel:
                            text: 'Статус: Активно'
                            size_hint: None, None
                        MDRaisedButton:
                            text: "Обновить данные"
                            md_bg_color: .35, .35, 1, 1
                            on_release:
                                app.resour()
                        MDCard:
                            size_hint: 0.485, None
                            size: 100, 200
                            pos_hint: {"center_x" : 0.5,"center_y": 0.5}
                            elevation: 10
                            padding: 25
                            spacing: 25
                            orientation: 'vertical'
                            MDLabel:
                                id: resource
                                text: 'Обновите данные'
                                halign: 'center'
                        MDCard:
                            size_hint: 0.485, None
                            size: 100, 200
                            pos_hint: {"center_x" : 0.5,"center_y": 0.5}
                            elevation: 10
                            padding: 25
                            spacing: 25
                            orientation: 'vertical'
                            MDLabel:
                                id: loadcpu
                                text: 'Обновите данные'
                                halign: 'center'
                        MDCard:
                            size_hint: 0.485, None
                            size: 100, 200
                            pos_hint: {"center_x" : 0.5,"center_y": 0.5}
                            elevation: 10
                            padding: 25
                            spacing: 25
                            orientation: 'vertical'
                            MDLabel:
                                id: model
                                text: 'Обновите данные'
                                halign: 'center'
                        MDCard:
                            size_hint: 0.485, None
                            size: 100, 200
                            pos_hint: {"center_x" : 0.5,"center_y": 0.5}
                            elevation: 10
                            padding: 25
                            spacing: 25
                            orientation: 'vertical'
                            MDLabel:
                                id: ipnatdetector
                                text: 'Обновите данные'
                                halign: 'center'
            #MDBottomNavigationItem:
                #name: 'Main_User'
                #text: 'Пользователи'
                #MDLabel:
                    #text: 'Активность пользователей'
                    #halign: 'center'
            MDBottomNavigationItem:
                name: 'Main_Settings'
                text: 'Настройки'
                ScrollView:
                    MDList:
                        id: setting
                        adaptive_height: True
                        OneLineIconListItem:
                            size: 100, 100
                            text: 'Интернет'
                            on_release:
                                app.root.current = 'Internet'
                            IconLeftWidget:
                                icon: "web"
                                on_release:
                                    app.root.current = 'Internet'
                        OneLineIconListItem:
                            size: 100, 100
                            text: 'Локальная сеть'
                            on_release:
                                app.root.current = 'Local'
                            IconLeftWidget:
                                icon: "router-network"
                                on_release:
                                    app.root.current = 'Local'
                        OneLineIconListItem:
                            size: 100, 100
                            text: 'Беспроводные сети'
                            on_release:
                                app.root.current = 'wifi'
                            IconLeftWidget:
                                icon: "radio-tower"
                                on_release:
                                    app.root.current = 'wifi'
                        OneLineIconListItem:
                            size: 100, 100
                            text: 'Проброс портов'
                            on_release:
                                app.root.current = 'Forward'
                            IconLeftWidget:
                                icon: "share-outline"
                                on_release:
                                    app.root.current = 'Forward'
                        OneLineIconListItem:
                            size: 100, 100
                            text: 'VPN'
                            on_release:
                                app.root.current = 'VPN'
                            IconLeftWidget:
                                icon: "key"
                                on_release:
                                    app.root.current = 'VPN'
                        OneLineIconListItem:
                            size: 100, 100
                            text: 'Дополнительные настройки'
                            on_release:
                                app.root.current = 'Other'
                            IconLeftWidget:
                                icon: "tune"
                                on_release:
                                    app.root.current = 'Other'
                        OneLineIconListItem:
                            size: 100, 100
                            text: 'Сведения и обновление'
                            on_release:
                                app.root.current = 'Update'
                            IconLeftWidget:
                                icon: "information"
                                on_release:
                                    app.root.current = 'Update'
<Internet>:
    MDBoxLayout:
        orientation: 'vertical'
        MDToolbar:
            title: 'MikroLite'
            md_bg_color: .35, .35, 1, 1
            specific_text_color: 1, 1, 1, 1
            right_action_items: [["exit-to-app", lambda x: app.logout()]]
        ScrollView:
            MDList:
                adaptive_height: True
                OneLineIconListItem:
                    size: 100, 50
                    text: 'Назад'
                    on_release:
                        app.root.current = 'MainMenu'
                    IconLeftWidget:
                        icon: "arrow-left"
                        on_release:
                            app.root.current = 'MainMenu'
                OneLineIconListItem:
                    text: 'Выберите тип подключения'
                    size: 100, 50
                OneLineIconListItem:
                    size: 100, 50
                    text: 'Автоматический (DHCP)'
                    MDLabel:
                        text: 'Приоритет:'
                        pos_hint: {'center_x': 1, 'center_y': .5}
                    MDTextField:
                        id: isp_dhcp_def
                        hint_text: "1,2,3,4"
                        width: 30
                        size_hint_x: None
                        pos_hint: {'center_x': 0.75, 'center_y': .5}
                    MDRaisedButton:
                        id: isp_dhcp_button
                        text: "Вкл"
                        md_bg_color: .35, .35, 1, 1
                        font_size: 14
                        size_hint_x: None
                        pos_hint: {'center_x': 0.9, 'center_y': .5}
                        on_release:
                            app.autoisp()
                            app.resour()
                OneLineIconListItem:
                    size: 100, 50
                    text: 'Статический'
                    on_release:
                        app.root.current = 'Static'
                OneLineIconListItem:
                    size: 100, 50
                    text: 'PPPoE'
                    on_release:
                        app.root.current = 'PPPoE'
                OneLineIconListItem:
                    size: 100, 50
                    text: 'LTE, WCDMA, GSM, USB-модем'
                    MDLabel:
                        text: 'Приоритет:'
                        pos_hint: {'center_x': 1.1, 'center_y': .5}
                    MDTextField:
                        id: isp_lte_def
                        hint_text: "1,2,3,4"
                        width: 30
                        size_hint_x: None
                        pos_hint: {'center_x': 0.79, 'center_y': .5}
                    MDLabel:
                        id: vsegda_activno
                        text: 'Всегда активно'
                        size_hint_x: None
                        pos_hint: {'center_x': 0.95, 'center_y': .5}
                OneLineIconListItem:
                    size: 100, 50
                    text: ''
                    MDRaisedButton:
                        id: save_prioritet
                        text: "Сохранить приоритеты"
                        md_bg_color: .35, .35, 1, 1
                        font_size: 14
                        size_hint_x: None
                        pos_hint: {'center_x': 0.85, 'center_y': .5}
                        on_release:
                            app.prioritet()
                OneLineIconListItem:
                    size: 100, 50
                    text: 'Межсетевой экран (Firewall)'
                    MDRaisedButton:
                        id: firewall
                        text: "Вкл"
                        md_bg_color: .35, .35, 1, 1
                        font_size: 14
                        size_hint_x: None
                        pos_hint: {'center_x': 0.9, 'center_y': .5}
                        on_release:
                            app.firewall()
