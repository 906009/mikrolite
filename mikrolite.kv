#:kivy 2.0.0
<Authorization>:
    MDCard:
        size_hint: None, None
        size: 770, 800
        pos_hint: {"center_x" : 0.5,"center_y": 0.5}
        elevation: 10
        padding: 25
        spacing: 25
        orientation: 'vertical'
        MDBoxLayout:
            orientation: 'vertical'
            pos_hint: {'center_x':.5}
            MDLabel:
                text: 'MikroLite\nАвторизация'
                halign: 'center'
            MDTextField:
                id: ip_dns
                hint_text: "IPv4 адрес или DNS имя"
                font_size: 40
            MDTextField:
                id: user
                hint_text: "Имя пользователя"
                font_size: 40
            MDTextField:
                id: passwordv
                hint_text: "Пароль"
                font_size: 40
                password: True
            MDBoxLayout:
                orientation: 'horizontal'
                MDRaisedButton:
                    text: "Подключиться"
                    md_bg_color: .35, .35, 1, 1
                    font_size: 36
                    on_release:
                        app.connector()
                        app.resour()
                        app.root.current = 'MainMenu'
                MDLabel:
                    size_hint: 0.1, None
                MDBoxLayout:
                    orientation: 'horizontal'
                    MDRaisedButton:
                        text: "Авто вход"
                        md_bg_color: .35, .35, 1, 1
                        font_size: 26
                        on_release:
                            app.standart()
                            app.resour()
                            app.root.current = 'MainMenu'
<MainMenu>:
    BoxLayout:
        orientation:'vertical'
        MDToolbar:
            title: 'MikroLite'
            md_bg_color: .35, .35, 1, 1
            specific_text_color: 1, 1, 1, 1
            right_action_items: [["exit-to-app", lambda x: app.logout()]]
        MDBottomNavigation:
            panel_color: .35, .35, 1, 1
            MDBottomNavigationItem:
                name: 'Main_Status'
                text: 'Статус'
                ScrollView:
                    GridLayout:
                        cols: 2
                        #orientation: 'vertical'
                        size_hint_y: None
                        height: self.minimum_height
                        padding: dp(48)
                        spacing: 10
                        MDLabel:
                            text: 'Статус: Активно'
                            size_hint: None, None
                        MDRaisedButton:
                            text: "Обновить данные"
                            md_bg_color: .35, .35, 1, 1
                            on_release:
                                app.resour()
                        MDCard:
                            size_hint: 0.485, None
                            size: 400, 600
                            pos_hint: {"center_x" : 0.5,"center_y": 0.5}
                            elevation: 10
                            padding: 25
                            spacing: 25
                            orientation: 'vertical'
                            MDLabel:
                                id: resource
                                text: 'Обновите данные'
                                halign: 'center'
                        MDCard:
                            size_hint: 0.9, None
                            size: 400, 600
                            pos_hint: {"center_x" : 0.5,"center_y": 0.5}
                            elevation: 10
                            padding: 25
                            spacing: 25
                            orientation: 'vertical'
                            MDLabel:
                                id: loadcpu
                                text: 'Обновите данные'
                                halign: 'center'
                        MDCard:
                            size_hint: 0.485, None
                            size: 400, 600
                            pos_hint: {"center_x" : 0.5,"center_y": 0.5}
                            elevation: 10
                            padding: 25
                            spacing: 25
                            orientation: 'vertical'
                            MDLabel:
                                id: model
                                text: 'Обновите данные'
                                halign: 'center'
                        MDCard:
                            size_hint: 0.9, None
                            size: 400, 600
                            pos_hint: {"center_x" : 0.5,"center_y": 0.5}
                            elevation: 10
                            padding: 25
                            spacing: 25
                            orientation: 'vertical'
                            MDLabel:
                                id: ipnatdetector
                                text: 'Обновите данные'
                                halign: 'center'
            #MDBottomNavigationItem:
                #name: 'Main_User'
                #text: 'Пользователи'
                #MDLabel:
                    #text: 'Активность пользователей'
                    #halign: 'center'
            MDBottomNavigationItem:
                name: 'Main_Settings'
                text: 'Настройки'
                ScrollView:
                    MDList:
                        id: setting
                        adaptive_height: True
                        OneLineIconListItem:
                            size: 100, 200
                            text: 'Интернет'
                            on_release:
                                app.root.current = 'Internet'
                                app.resour_ispnet()
                            IconLeftWidget:
                                icon: "web"
                                on_release:
                                    app.root.current = 'Internet'
                                    app.resour_ispnet()
                        OneLineIconListItem:
                            size: 100, 200
                            text: 'Локальная сеть'
                            on_release:
                                app.root.current = 'Local'
                                app.resour_localnet()
                            IconLeftWidget:
                                icon: "router-network"
                                on_release:
                                    app.root.current = 'Local'
                                    app.resour_localnet()
                        OneLineIconListItem:
                            size: 100, 200
                            text: 'Беспроводные сети'
                            on_release:
                                app.root.current = 'wifi'
                                app.resour_wifi()
                            IconLeftWidget:
                                icon: "radio-tower"
                                on_release:
                                    app.root.current = 'wifi'
                                    app.resour_wifi()
                        OneLineIconListItem:
                            size: 100, 200
                            text: 'IPv6'
                            on_release:
                                app.root.current = 'IPv6'
                                app.IPv6()
                            IconLeftWidget:
                                icon: "numeric-6-box-multiple"
                                on_release:
                                    app.root.current = 'IPv6'
                                    app.IPv6()
                        OneLineIconListItem:
                            size: 100, 200
                            text: 'Проброс портов'
                            on_release:
                                app.root.current = 'Forward'
                            IconLeftWidget:
                                icon: "share-outline"
                                on_release:
                                    app.root.current = 'Forward'
                        OneLineIconListItem:
                            size: 100, 200
                            text: 'VPN'
                            on_release:
                                app.root.current = 'VPN'
                            IconLeftWidget:
                                icon: "key"
                                on_release:
                                    app.root.current = 'VPN'
                        OneLineIconListItem:
                            size: 100, 200
                            text: 'Дополнительные настройки'
                            on_release:
                                app.root.current = 'Other'
                            IconLeftWidget:
                                icon: "tune"
                                on_release:
                                    app.root.current = 'Other'
                        OneLineIconListItem:
                            size: 100, 200
                            text: 'Docker'
                            on_release:
                                app.root.current = 'Docker'
                            IconLeftWidget:
                                icon: "docker"
                                on_release:
                                    app.root.current = 'Docker'
